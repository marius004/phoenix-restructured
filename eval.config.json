{
    "isolatePath": "/usr/local/bin/isolate",
    "maxSandboxes": 10,
    "compilePath": "/tmp/pn-compile",
    "outputPath": "/tmp/pn-output",

    "languages": {
        "c": {
          "extension": ".c",
          "isCompiled": true,
          "compile": [
            "/bin/gcc",
            "-std=c11",
            "-O2",
            "-s",
            "-static",
            "main.c",
            "-o",
            "main"
          ],
          "execute": [
            "./main"
          ],
          "sourceFile": "box/main.c",
          "executable": "box/main"
        }
    }
}